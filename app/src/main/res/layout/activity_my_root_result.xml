<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/result_constraint"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".myroot.MyRootResultActivity"
    tools:layout_editor_absoluteX="-7dp"
    tools:layout_editor_absoluteY="0dp">

    <!--상단 보라색 바-->
    <View
        android:id="@+id/view13"
        android:layout_width="415dp"
        android:layout_height="92dp"
        android:background="@color/찐보라"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <!--가운데 보라 막대-->
    <View
        android:layout_width="193dp"
        android:layout_height="37dp"
        android:background="#594C61"
        android:layout_marginTop="230dp"
        android:layout_marginLeft="102dp"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.325" />

    <!--가운데 원-->
    <View
        android:id="@+id/view18"
        android:layout_width="96dp"
        android:layout_height="90dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="200dp"
        android:background="@drawable/rounded_dialog"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <!--가운데 원-->
    <View
        android:layout_width="96dp"
        android:layout_height="91dp"
        android:layout_marginStart="257dp"
        android:layout_marginTop="200dp"
        android:background="@drawable/rounded_dialog"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <!--출발역명 위 출발역텍스트-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="출발역"
        android:textSize="15dp"
        android:textColor="#3F3240"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.243"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.017" />

    <!--도착역명 위 도착역텍스트-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="도착역"
        android:textSize="15dp"
        android:textColor="#3F3240"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.727"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.016" />

    <!--뒤로가기버튼-->
    <ImageView
        android:id="@+id/IB_goback"
        android:layout_width="42dp"
        android:layout_height="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/goback" />

    <!--상단 화살표 아이콘-->
    <ImageView
        android:layout_width="66dp"
        android:layout_height="76dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024"
        app:srcCompat="@drawable/move" />

    <!--상단 출발역명 텍스트-->
    <TextView
        android:id="@+id/tv_dStation"
        android:layout_width="76dp"
        android:layout_height="33dp"
        android:text="출발역명"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="78dp"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.325"
        tools:ignore="MissingConstraints"/>

    <!--가운데 원 안 출발역 텍스트-->
    <TextView
        android:id="@+id/tv_dStation2"
        android:layout_width="95dp"
        android:layout_height="48dp"
        android:gravity="center"
        android:text="출발역"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#525252"
        android:layout_marginTop="220dp"
        android:layout_marginLeft="46dp"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.325"
        tools:ignore="MissingConstraints"/>

    <!--가운데 원 안 도착역텍스트-->
    <TextView
        android:id="@+id/tv_aStation2"
        android:layout_width="94dp"
        android:layout_height="41dp"
        android:gravity="center"
        android:text="도착역"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#525252"
        android:layout_marginTop="225dp"
        android:layout_marginLeft="258dp"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.325"
        tools:ignore="MissingConstraints"/>

    <!--depart텍스트-->
    <TextView
        android:layout_width="94dp"
        android:layout_height="41dp"
        android:gravity="center"
        android:text="depart!"
        android:textSize="15dp"
        android:textStyle="bold"
        android:layout_marginTop="168dp"
        android:layout_marginLeft="45dp"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.325"
        tools:ignore="MissingConstraints"/>

    <!--arrive텍스트-->
    <TextView
        android:layout_width="94dp"
        android:layout_height="41dp"
        android:gravity="center"
        android:text="arrive"
        android:textSize="15dp"
        android:textStyle="bold"
        android:layout_marginTop="168dp"
        android:layout_marginLeft="258dp"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.325"
        tools:ignore="MissingConstraints"/>

    <!--상단 도착역명-->
    <TextView
        android:id="@+id/tv_aStation"
        android:layout_width="81dp"
        android:layout_height="34dp"
        android:text="도착역명"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="259dp"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.325"
        tools:ignore="MissingConstraints"/>

    <!--차 아이콘-->
    <ImageView
        android:layout_width="89dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/going"
        android:layout_marginTop="177dp"
        android:layout_marginLeft="140dp"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.325"
        tools:ignore="MissingConstraints"/>

    <!--결과창-->
    <TextView
        android:id="@+id/tv_routeResult"
        android:layout_width="351dp"
        android:layout_height="343dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="330dp"
        android:background="@drawable/rounded_result"
        android:gravity="center"
        android:text="결과창"
        android:textColor="#302B36"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.103" />


</androidx.constraintlayout.widget.ConstraintLayout>